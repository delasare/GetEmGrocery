#jqt ul,
#jqt ul li {
	background-color:transparent;
	background-image:none;
	color:#000;
	text-shadow:none;
	border:none;
	font-family: "postino";
	font-size:13pt;
	}

#jqt ul li a.active {
	background-color:transparent;
	}
	
#scrollWrapper,
#recipeScrollWrapper {
	height:480px; 
	}

#recipeList,
#fullRecipeTxt {
	height:auto;
	}	

#recipe ul {
/*	margin-top:70px;*/
	}

#recipe ul li,
#fullRecipe ul li {
    clear:both;
}


#recipe ul span.amount,
#fullRecipe ul span.amount{
	display:inline-block;
	width:100px;
    overflow:hidden;
   /* height:1.5em;*/
	}

#recipe ul span.food,
#fullRecipe ul span.food{
    display:inline-block;
	width:150px;
	vertical-align:top;
	background-image:url(../img/dot.png);
	background-position:top left;
	background-repeat:no-repeat;
	-webkit-background-size: 21px 20px;
	padding:5px 0 5px 5px;
	}

#fullRecipe ul span.amount{width:110px;}
#fullRecipe ul span.food{width:140px;}

#recipe ul img.clueBtn  {
	display:inline-block;
	vertical-align:middle;
	}

#recipe ul img.clueBtn {
	width: 155px;
	height: 50px;
    position:relative;
    left:10px;
	}

#recipe ul li.unsolved {
	padding:0 10px;
	}


#recipe ul li.solved,
#fullRecipe ul li.solved{
	padding:10px 10px 0 10px;
	}

